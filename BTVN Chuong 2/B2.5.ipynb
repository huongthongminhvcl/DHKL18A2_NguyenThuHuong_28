   "source": [
    "from xml.dom import minidom\n",
    "\n",
    "# Tải file XML\n",
    "doc = minidom.parse(\"sample.xml\")\n",
    "\n",
    "# Lấy tất cả các thẻ <name>\n",
    "names = doc.getElementsByTagName(\"name\")\n",
    "\n",
    "print(\"Danh sách các phần tử <name> trong file:\")\n",
    "for n in names:\n",
    "    print(\"-\", n.firstChild.data)\n"
   ]
  }
